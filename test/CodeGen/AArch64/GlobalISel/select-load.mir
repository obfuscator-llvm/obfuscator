# CHECK-NEXT:  - { id: 1, class: gpr64, preferred-register: '' }
registers:
  - { id: 0, class: gpr }
  - { id: 1, class: gpr }

stack:
  - { id: 0, name: ptr0, offset: 0, size: 8, alignment: 8 }

# CHECK:  body:
# CHECK: %1 = LDRXui %stack.0.ptr0, 0 :: (load 8)
# CHECK: %x0 = COPY %1
body:             |
  bb.0:
    liveins: %x0

    %0(p0) = G_FRAME_INDEX %stack.0.ptr0
    %1(s64) = G_LOAD %0 :: (load 8)
    %x0 = COPY %1(s64)
...

